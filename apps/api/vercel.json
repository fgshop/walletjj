{
  "framework": null,
  "buildCommand": "bash scripts/vercel-build.sh",
  "outputDirectory": ".",
  "functions": {
    "api/index.js": {
      "maxDuration": 60,
      "includeFiles": "node_modules/.prisma/**"
    }
  },
  "rewrites": [
    { "source": "/v1/(.*)", "destination": "/api" },
    { "source": "/docs", "destination": "/api" },
    { "source": "/(.*)", "destination": "/api" }
  ]
}
